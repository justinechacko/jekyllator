<div class="container">
  <%= form_tag(blog_path, method: :put ) do %>
    <%= label_tag :title %>:<br />
    <%= text_field_tag :title, @post["title"] %><br />

    <%= label_tag :content %>:<br />
    <%= text_area_tag :content,@post["content"],class: "contents"%><br />
    <%= submit_tag("Submit") %>
  <% end %>

</div>
